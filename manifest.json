{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "resources/jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background/background.js",
      "background/trafficInspector.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "inject/*.js"
  ],
  "devtools_page": "panel/panel.html"
}